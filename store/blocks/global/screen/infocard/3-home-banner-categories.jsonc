    "children": [
      "image#banner-categories__phones--image",
      "image#banner-categories__phones--image-mobile",
      "rich-text#banner-categories__phones--title",
      "flex-layout.row#banner-categories__phones--container-overlay"
    ]
  },

  "image#banner-categories__phones--image": {
    "title": "Category Phones Image",
    "props": {
      "src": "https://stockimages.tiendasd1.com/stockimages.tiendasd1.com/kobastockimages/IMAGENES/12002531.png",
      "link": {
        "url": "/techo"
      },
      "blockClass": "banner-categories__phones--image"
    }
  },

  "image#banner-categories__phones--image-mobile": {
    "title": "Category Phones Image",
    "props": {
      "src": "https://stockimages.tiendasd1.com/stockimages.tiendasd1.com/kobastockimages/IMAGENES/12002531.png",
      "link": {
        "url": "/techo"
      },
      "blockClass": "banner-categories__phones--image-mobile"
