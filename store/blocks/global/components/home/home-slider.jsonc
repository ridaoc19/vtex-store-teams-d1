      "image#home__slider--cuatro",
      "image#home__slider--cinco",
      "image#home__slider--seis"
    ]
  },
  "image#home__slider--uno": {
    "props": {
      "src": "https://d1.com.co/wp-content/uploads/Banners_Panini_Corp_2022.png"
    }
  },
  "image#home__slider--dos": {
    "props": {
      "src": "https://d1.com.co/wp-content/uploads/BannerUshikoPlataformas_Corp.png"
    }
  },
  "image#home__slider--tres": {
    "props": {
