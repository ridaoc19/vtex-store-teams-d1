  "modal-trigger#home__modal--trigger": {
    "title": "home__modal--trigger",
    "children": [
      "product-summary-space",
      "product-summary-image",
      "product-summary-space",
      "product-summary-price",
      "product-summary-name",
      "product-summary-space",
      "add-to-cart-button",
      "modal-layout#home__modal--cart"
    ],
    "props": {
      "blockClass": "home__shelf"
    }
  },

  "product-summary-image": {
    "title": "summary-image",
    "props": {
      "width": "130%"
    }
  },
  "add-to-cart-button": {
    "title": "cart-button",
    "props": {
      "text": "Agregar ðŸ›’",
      "icon": true,
      "blockClass": "home__boton"
    }
  },
  "modal-layout#home__modal--cart": {
    "title": "home__modal--cart",
    "children": [
      "modal-header#modal__cart--header",
      "modal-content#modal__cart--content"
    ],
    "props": {
      "blockClass": "home__modal--cart"
    }
  },
  "modal-header#modal__cart--header": {
    "title": "modal__cart--header",
    "props": {
      "blockClass": "modal__cart--header"
    }
  },
  "modal-content#modal__cart--content": {
    "title": "modal__cart--content",
    "children": [
      "flex-layout.row#modal__cart--container"
    ]
  },
  "flex-layout.row#modal__cart--container": {
    "title": "modal__cart--container",
    "children": [
      "flex-layout.col#modal__cart--images",
      "flex-layout.col#modal__cart--detail"
    ],
    "props": {
      "blockClass": "modal__cart--container"
    }
  },
  "flex-layout.col#modal__cart--images": {
    "title": "modal__cart--images",
    "children": [
      "product-images#cart__images"
    ],
    "props": {
      "blockClass": "modal__cart--images"
    }
  },
  "product-images#cart__images": {
    "title": "cart__images",
    "props": {
      "showNavigationArrows": false,
      "showPaginationDots": false,
      "maxHeight": 200,
      "blockClass": "cart__images"
    }
  },
  "flex-layout.col#modal__cart--detail": {
    "title": "modal__cart--detail",
    "children": [
      "product-summary-name#modal__cart--nem",
      "product-price#modal__cart--price",
      "add-to-cart-button",
      "product-summary-space"
    ],
    "props": {
      "blockClass": "modal__cart--detail"
    }
  },
  "product-summary-name#modal__cart--nem": {
    "title": "modal__cart--nem",
    "props": {
      "blockClass": "modal__cart--nem"
    }
  },
  "product-price#modal__cart--price": {
    "title": "modal__cart--price",
    "props": {
      "blockClass": "modal__cart--price"
    }
  }
}