{
  "list-context.product-list#demo1": {
    "title": "vitrina alimentos y despensa",
    "blocks": [
      "product-summary.shelf#home__slider--chelf"
    ],
    "children": [
      "flex-layout.row#home__shelf--title",
      "slider-layout#demo-products"
    ]
  },
  "product-summary.shelf#home__slider--chelf": {
    "children": [
      "product-summary-image",
      "product-summary-price",
      "product-summary-name",
      "product-summary-description",
      // "product-summary-sku-selector",
      "product-summary-space",
      "product-summary-buy-button",
      "add-to-cart-button"
    ]
  },
  "slider-layout#demo-products": {
    "props": {
      // "itemsPerPage": {
      //   "desktop": 7,
      //   "tablet": 7,
      //   "phone": 7
      // },
      "infinite": false,
      "showNavigationArrows": "never",
      "blockClass": "carousel",
      "usePagination": false

    }
  },
  // FLEX LAYOUT TITULOS
  "flex-layout.row#home__shelf--title": {
    "title": "texto titulo",
    "children": [
      "flex-layout.col#home__flex--title",
      "flex-layout.col#home__flex--ver"
    ],
    "props": {
      "blockClass": "home__flex--manzana"
    }
  },
  "flex-layout.col#home__flex--title": {
    "children": [
      "rich-text#home__rich--title"
    ]
  },
  "flex-layout.col#home__flex--ver": {
    "children": [
      "link.product#home_link--ver"
    ],
    "props": {
      "horizontalAlign": "right"
    }
  },
  "rich-text#home__rich--title": {
    "props": {
      "text": "Alimentos y Despensa"
    }
  },
  "link.product#home_link--ver": {
    "props": {
      "href": "/{slug}/p",
      "label": "Ver mÃ¡s ->"
    }
  },
  "product-summary-buy-button": {
    "props": {
      "buyButtonText": "Agregar",
      "text": "Agregar"
    }
  }
}
